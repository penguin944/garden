<p id="notice"><%= notice %></p>

<h1>Listing Feeds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each unless @feeds.nil? do |feed| %>
      <tr>
        <td><%= feed.name %></td>
        <td><%= link_to 'Show', feed %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>